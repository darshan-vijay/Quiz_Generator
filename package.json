{
  "name": "capstone-starter",
  "version": "1.0.0",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "collect": "node build/collect.js",
    "analyze": "node build/analyze.js",
    "migrate": "knex migrate:latest --knexfile databases/knexfile.js",
    "build": "tsc",
    "clean": "rm -rf ./build",
    "watch": "nodemon src/app.ts",
    "test": "tsc --noEmit && vitest run",
    "dev:server": "nodemon --watch 'src/webSupport/**/*.ts' --exec 'ts-node --transpile-only' src/webSupport/appServer.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "@types/dotenv": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  }
}
