{
  "name": "capstone-starter",
  "version": "1.0.0",
  "main": "buils/app.js",
  "scripts": {
    "start": "node build/app.js",
    "collect": "node build/collect.js",
    "analyze": "node build/analyze.js",
    "migrate": "knex migrate:latest --knexfile databases/knexfile.js",

    "build": "tsc && cp -r src/views src/public build/",
    "clean": "rm -rf ./build",
    "watch": "nodemon src/app.ts",
    "test": "tsc --noEmit && vitest run"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^4.21.2",
    "express-handlebars": "^8.0.1",
    "knex": "^3.1.0",
    "pg": "^8.13.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  }
}
